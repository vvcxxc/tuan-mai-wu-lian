.coupon-box
  width 100%
  height 276px
  background: #ffffff
  border-radius: 16px
  padding: 30px
  display: flex
  box-sizing: border-box
  margin-bottom: 16px
  .coupon-img
    width 216px
    height 216px
    border-radius: 16px
    margin-right: 22px
  .coupon-detail
    flex: 1
    position relative
    .coupon-title
      width 100%
      // max-height: 61px
      line-height: 38px
      font-size: 26px
      position: relative
      overflow : hidden
      text-overflow: ellipsis
      display: -webkit-box
      -webkit-line-clamp: 2
      -webkit-box-orient: vertical
      .coupon-type
        display inline-block
        vertical-align: 2px
        background: #ff4444
        border-radius: 6px
        padding: 4px 8px
        line-height: 1
        font-size: 20px
        color: #ffffff
        margin-right: 4px
        margin-bottom: 4px
    .coupon-validity
      font-size: 24px
      color: #999999
      margin-top: 10px
