    .money-box
      position: absolute
      bottom: 0
      .member-box
        display: flex
        align-items: baseline
        .member-name
          color:#FF4444
          font-size: 20px
          line-height: 1
        .member-money
          color:#FF4444
          font-size: 32px
          font-weight: bold
          line-height: 1
      .member-label
        width auto
        height 39px
        display: flex
        padding: 0 13px
        background: #333433
        color: #EDD1A6
        font-size: 20px
        align-items: center
        border-radius: 17px
        margin-top: 13px
        .goto
          width 18px
          height 18px
          margin-left: 8px
      .member-label1
        width auto
